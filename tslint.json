{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "interface-name": [false],
        "trailing-comma": [false],
        "ban-types": [true, ["Object", "Use 'object' instead."], ["String", "Use 'string' instead"]],
        "member-access": [false],
        "array-type": [false]
    },
    "rulesDirectory": []
}