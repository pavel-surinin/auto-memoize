{
  "name": "auto-memoize",
  "version": "1.0.9",
  "description": "Memoize function with predifined caching strategies",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pavel-surinin/auto-memoize"
  },
  "keywords": ["memoize", "cache", "functional", "javascript", "typescript"],
  "badges": [
    {
      "description": "Travis CI build",
      "href": "https://travis-ci.com/pavel-surinin/auto-memoize",
      "url": "https://travis-ci.com/pavel-surinin/auto-memoize.svg?branch=master"
    },
    {
      "description": "Code coverage",
      "href": "https://coveralls.io/github/pavel-surinin/auto-memoize?branch=master",
      "url": "https://coveralls.io/repos/github/pavel-surinin/auto-memoize/badge.svg?branch=master"
    }
  ],
  "bugs": {
    "url": "https://github.com/pavel-surinin/auto-memoize/issues",
    "email": "pavel.surinin@gmail.com"
  },
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "test:coverage": "npm test -- --coverage",
    "delete:target": "rm -rf target",
    "babel": "./node_modules/.bin/babel ./target/js-ts --out-dir ./target/js-babel",
    "build": "npm run delete:target && npm run lint && npm run compile && npm run test:coverage && npm run babel",
    "copy": "copy.sh",
    "publish:patch": "npm run build && npm version patch && npm run copy && npm publish target/js-babel",
    "publish:minor": "npm run build && npm version minor && npm run copy && npm publish target/js-babel",
    "publish:major": "npm run build && npm version major && npm run copy && npm publish target/js-babel",
    "coveralls": "cat target/coverage/lcov.info | coveralls"
  },
  "author": "Pavel Surinin <pavel.surinin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@babel/cli": "7.2.3",
    "benchmark": "2.1.4",
    "coveralls": "3.0.2",
    "jest": "24.4.0",
    "ts-jest": "24.0.0",
    "tslint": "5.14.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/core": "7.2.2",
    "fast-deep-equal": "2.0.1"
  }
}
